{
    "SMDVersion":"4.0",
    "id":"policy.smd.json",
    "description":"Policy services descriptor",
    "identity":[
        "notWorkingIdentity=${notWorkingIdentity};otherId=${otherId}",
        "policyId=${policyId};otherId=${otherId}"
    ],
    "target":"/_mocks/schemaStore/data/Policy/",
    "methods":{
        "query":[
            {
                "emptyQueryAllowed":true,
                "techName":"SearchTest",
                "displayName":null
            }
        ],
        "get":[
            {
                "techName":"GetTest",
                "displayName":null,
                "templates":{
                    "default":"eq(foo,*)"
                }
            }
        ],
        "add":[
            {
                "techName":"AddTest",
                "displayName":null,
                "schema":{
                    "additionalProperties":false,
                    "properties":{
                        "dateBegin":{
                            "description":"Policy beginning effective date",
                            "format":"date-time",
                            "type":"string",
                            "required":true
                        },
                        "version":{
                            "type":"integer",
                            "required":false
                        }
                    }
                }
            }
        ]
    },
    "subordinates":{
        "Version":{
            "id":"policyVersionSmd",
            "description":"Policy version services descriptor",
            "identity":"versionId",
            "methods":{
                "query":[
                    {
                        "emptyQueryAllowed":true,
                        "techName":"SearchTestVersion",
                        "displayName":null
                    }
                ],
                "get":[
                    {
                        "techName":"GetTestVersion",
                        "displayName":null
                    }
                ]
            }
        },
        "Intervention":{
            "id":"policyInterventionSmd",
            "description":"Policy intervention services descriptor",
            "identity":"interventionId",
            "methods":{
                "get":[
                    {
                        "techName":"GetTestIntervention",
                        "displayName":null
                    }
                ]
            }
        }
    }
}